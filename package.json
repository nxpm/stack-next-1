{
  "name": "nxpm",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build api",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps --base=remotes/origin/main",
    "affected:libs": "nx affected:libs --base=remotes/origin/main",
    "affected:build": "nx affected:build --base=remotes/origin/main",
    "affected:e2e": "nx affected:e2e --base=remotes/origin/main",
    "affected:test": "nx affected:test --base=remotes/origin/main",
    "affected:lint": "nx affected:lint --base=remotes/origin/main",
    "affected:dep-graph": "nx affected:dep-graph --base=remotes/origin/main",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-generator": "nx workspace-generator",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "test:ci": "yarn nx affected:test && yarn nx affected:e2e"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn format:check"
    }
  },
  "lint-staged": {
    "*.{js,json,css,scss,less,md,ts,html,graphql}": [
      "yarn format --uncommitted"
    ],
    "nx.json": [
      "yarn nxpm-stack lint",
      "yarn format:write"
    ]
  },
  "private": true,
  "dependencies": {},
  "devDependencies": {
    "@nrwl/angular": "^11.1.5",
    "@nrwl/cli": "11.1.5",
    "@nrwl/devkit": "11.1.5",
    "@nrwl/eslint-plugin-nx": "11.1.5",
    "@nrwl/jest": "11.1.5",
    "@nrwl/nest": "^11.1.5",
    "@nrwl/nx-plugin": "^11.1.5",
    "@nrwl/tao": "11.1.5",
    "@nrwl/workspace": "11.1.5",
    "@nxpm/cli": "^4.4.6",
    "@nxpm/stack": "^4.4.6",
    "@types/jest": "26.0.8",
    "@types/node": "12.12.38",
    "@typescript-eslint/eslint-plugin": "4.3.0",
    "@typescript-eslint/parser": "4.3.0",
    "dotenv": "6.2.0",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.0.0",
    "husky": "^4.3.7",
    "jest": "26.2.2",
    "lint-staged": "^10.5.3",
    "prettier": "^2.2.1",
    "ts-jest": "26.4.0",
    "ts-node": "~9.1.1",
    "tslib": "^2.0.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.3"
  }
}
